<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ayprofes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_ay"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ay"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManejoProfesores.ProfesorActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManejoUsuarios.LoginActivity" />
        <activity android:name=".ManejoProfesores.ComentarioActivity" />
        <activity android:name=".ManejoProfesores.BuscarActivity" />
        <activity android:name=".ManejoUsuarios.RecoverActivity" />
        <activity android:name=".ManejoUsuarios.SignUpActivity" />
        <activity android:name=".ManejoProfesores.NuevoProfeActivity" />
    </application>

</manifest>